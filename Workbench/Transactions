-- SELECT * from ACCESS_VIEWS.DW_LSTG_ITEM 
-- where ITEM_ID = 303361317172


select t2.AUCT_TITL, t1.ITEM_PRICE, t1.TOTAL_AMOUNT, t1.CREATED_DT
from ACCESS_VIEWS.DW_CHECKOUT_TRANS as t1
inner join ACCESS_VIEWS.DW_LSTG_ITEM as t2
on t1.ITEM_ID = t2.ITEM_ID
where BUYER_ID = 531317492