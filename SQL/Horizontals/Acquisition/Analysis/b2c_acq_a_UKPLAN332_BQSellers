select *
from 
(SELECT distinct
v.seller_id,
U.USER_SLCTD_ID,
C.CNTRY_DESC,
v.seller_sec_vat AS VAT_ID,
v.VAT_Region,
v.status_type AS VAT_Status,
v.DEFAULT_REGION_FLAG AS OSS_Flag,
case WHEN vat.cbt_dec IS NULL THEN FALSE ELSE vat.cbt_dec end AS CBT_Flag,
vf.first_name,
vf.last_name,
vf.business_name,
vf.addr_line1,
vf.addr_line2,
vf.city_cd,
vf.state_cd,
vf.zip_cd,
vf.extrnl_res_cd AS TA_Error_Code,
vf.extrnl_res_txt AS TA_Error_Code_Description,
vf.map_error_msg Internal_Description
,case v.STATUS_TYPE when 'VERIFIED' then 1
when 'VALID_NOT_OWNED' then 2 else 3 end as VAT_RANK
,row_number() over (partition by VAT_ID order by VAT_RANK) as vat_hierarchy
FROM PRS_SECURE_V.lstg_vat_record v
LEFT JOIN PRS_SECURE_V.DW_USERS U ON v.seller_id=U.user_id
LEFT JOIN dw_slng_user_vat_service vat ON v.SELLER_ID=vat.seller_id
LEFT JOIN ACCESS_VIEWS.DW_COUNTRIES C ON u.USER_CNTRY_ID=C.cntry_id
LEFT JOIN PRS_SECURE_V.lstg_vat_verification vf ON v.lst_ver_audit_id = vf.verif_audit_id
-- WHERE UPPER(v.vat_region) IN (
-- 'AT','BE','BG','CY','CZ','DE','DK','EE','ES','FI','FR','GR','HR','HU','IE','IT','LT','LU','LV','MT','NL','PL','PT','RO','SE','SI','SK','EL','XI')
WHERE 1=1
